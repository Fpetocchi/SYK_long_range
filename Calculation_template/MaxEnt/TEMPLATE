# LAUNCH FROM PATH_DATA

#for it in `seq 50 1 50`


declare -a dirindex=( "36_siCorr" "50_noCorr" )
for it in "${dirindex[@]}" ;
do
   cd $it/Convergence
   cp ../../../../MaxEnt/MaxEnt.sh .
   chmod 700 MaxEnt.sh

   ./MaxEnt.sh -e 0.005 -w 8000 -W 150 -f G -o 1 -i lat
   ./MaxEnt.sh -e 0.005 -w 8000 -W 150 -f G -o 1 -i qmc_Ti
   ./MaxEnt.sh -e 0.005 -w 8000 -W 150 -f G -o 2 -i qmc_Ti
   ./MaxEnt.sh -e 0.005 -w 8000 -W 150 -f G -o 3 -i qmc_Ti
   ./MaxEnt.sh -e 0.01  -w 8000 -W 300 -f W -o 1 -i lat

   cd ../../

done
