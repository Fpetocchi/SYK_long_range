import collections as cllct
import numpy as np
#
#
# --------------------------------------------------------------------------- #
Nat = 156

Els = ["Ta1","Ta2","Ta3","Ta4","Ta5","Ta6","Ta7","Ta8","Se"]
#
allDoS = cllct.defaultdict(dict)
counts = cllct.defaultdict(dict)
orbs = cllct.defaultdict(dict)
#
for i in range(1,9):
    orbs["Ta%s"%i] = {"1":"s","2":"s","3":"p","4":"d"}
orbs["Se"] = {"1":"s","2":"p"}
#
for El in Els:
    for no,ko in orbs[El].items():
        #
        print("reading %s %s,%s"%(El,no,ko))
        allDoS[El]["%s_%s"%(no,ko)] = np.zeros(3639)
        counts[El]["%s_%s"%(no,ko)] = 0
        #
        for iat in range(1,Nat+1):
            #
            file = "1T-TaSe2_cdw.pdos_atm#%s(%s)_wfc#%s(%s)"%(iat,El,no,ko)
            #
            try:
                w,Pdos = np.genfromtxt( file, dtype='double', usecols=(0,1), unpack=True, comments='#' )
                allDoS[El]["%s_%s"%(no,ko)] += Pdos
                counts[El]["%s_%s"%(no,ko)] += 1
            except:
                #print("%s not found"%file)
                continue

for El in Els:
    for no,ko in orbs[El].items():
        if counts[El]["%s_%s"%(no,ko)]!=0:
            allDoS[El]["%s_%s"%(no,ko)] /= counts[El]["%s_%s"%(no,ko)]
            np.savetxt("../DoS/DoS_%s_%s%s.DAT"%(El,no,ko),np.c_[w,allDoS[El]["%s_%s"%(no,ko)]])




