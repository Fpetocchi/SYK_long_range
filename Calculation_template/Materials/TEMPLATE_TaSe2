a=1
b=200
for kpl in `seq ${a} ${b}`; do
   #
   kp=`LC_ALL=en awk "BEGIN{print ${kpl} + 200 }"`
   #
   echo checking $kp
   if [ ! -f MaxEnt_Gk_path_s1/Gk_t_k${kp}.DAT_dos.dat ]; then
      kp_copy=`LC_ALL=en awk "BEGIN{print 200 - ${kpl} + 1 }"`
      echo copying_ $kp_copy
      cp MaxEnt_Gk_path_s1/Gk_t_k${kp_copy}.DAT_dos.dat MaxEnt_Gk_path_s1/Gk_t_k${kp}.DAT_dos.dat
   fi
done






