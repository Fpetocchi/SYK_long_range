<?xml version="1.0" encoding="UTF-8"?>
<fortpy mode="docstring">
  <decorates name="numerical_utilities.equal_rank1">
    <group name="equal_rank1" purpose="testing" staging="../unittests/">
      <test identifier="original" description="Tests the equality of rank 1 floats." timed="true"
	    cases="[1-51]">
	<output identifier="equal_rank1.out" folder="../tests/numerical_utilities"
		file="equal_rank1.out.{}" template="string.xml" />
	<target name="[default]" compareto="equal_rank1.out" varfile="equal_rank1.out"
		when="each" />
      </test>
      <assignment name="a" value="ain" allocate="true"> 
	<value identifier="ain" folder="../tests/numerical_utilities" file="equal_rank1_a.in.{}"
	       rename="equal_rank1_a.in"/>
      </assignment>
      <assignment name="b" value="bin" allocate="true"> 
	<value identifier="bin" folder="../tests/numerical_utilities" file="equal_rank1_b.in.{}"
	       rename="equal_rank1_b.in"/>
      </assignment>
      <assignment name="rtolerance" value="rtol">
	<value identifier="rtol" folder="../tests/numerical_utilities"
	       file="equal_rank1_tol.in.{}" rename="equal_rank1_tol.in"/> 
      </assignment>
      <assignment name="atolerance_" constant="1E-8"> 
      </assignment>
    </group>
  </decorates>

